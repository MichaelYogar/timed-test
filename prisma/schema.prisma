generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Question {
  id        Int      @id @default(autoincrement())
  content   String   @db.VarChar(255)
  duration  Int
  createdAt DateTime @default(now()) @db.Timestamp(6)
}